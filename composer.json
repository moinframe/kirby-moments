{
    "name": "femundfilou/kirby-moments",
    "description": "Little Moments Matter. Share Yours.",
    "license": "MIT",
    "homepage": "https://plugins.femundfilou.com/kirby-moments",
    "type": "kirby-plugin",
    "version": "1.0.0-beta.6",
    "authors": [
        {
            "name": "Justus Kraft",
            "email": "justus@femundfilou.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Femundfilou\\Moments\\": "lib/"
        }
    },
    "require": {
        "getkirby/composer-installer": "^1.1",
        "getkirby/cms": "^4.0"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --config .php-cs-fixer.dist.php"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.37"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}